{
  "kind": "collectionType",
  "collectionName": "channels",
  "info": {
    "singularName": "channel",
    "pluralName": "channels",
    "displayName": "channel",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title_channel": {
      "type": "string",
      "required": false
    },
    "image_channel": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "subtitle_channel": {
      "type": "string",
      "required": false
    },
    "meta_title_channel": {
      "type": "text",
      "required": false
    },
    "meta_description_channel": {
      "type": "text",
      "required": false
    },
    "slug_channel": {
      "type": "uid",
      "targetField": "title_channel",
      "required": false
    },
    "diaries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::diary.diary",
      "mappedBy": "channels"
    },
    "meta_tags": {
      "type": "component",
      "repeatable": true,
      "component": "meta-tags.tags"
    },
    "description_main": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
